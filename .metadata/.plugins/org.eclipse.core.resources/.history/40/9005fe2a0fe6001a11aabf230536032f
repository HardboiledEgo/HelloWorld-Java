class LogicalOPtable {
	public static void main(String args[]) {
		double p, q;
		
		System.out.println("P\tQ\tAND\tOR\tXOR\tNOT");
		
		p = 1; q = 1;
		System.out.print(p + "\t" + q + "\t");
		System.out.print((p * q) + "\t" + (int) ((p + q)/2 + 0.5) + "\t");
		System.out.println((p + q) + "\t" + Math.abs(p - 1));
		
		p = 1; q = 0;
		System.out.print(p + "\t" + q + "\t");
		System.out.print((p * q) + "\t" + (int) ((p + q)/2 + 0.5) + "\t");
		System.out.println((p + q) + "\t" + Math.abs(p - 1));
		
		p = 0; q = 1;
		System.out.print(p + "\t" + q + "\t");
		System.out.print((p * q) + "\t" + (int) ((p + q)/2 + 0.5) + "\t");
		System.out.println((p + q) + "\t" + Math.abs(p - 1));
		
		p = 0; q = 0;
		System.out.print(p + "\t" + q + "\t");
		System.out.print((p * q) + "\t" + (int) ((p + q)/2 + 0.5) + "\t");
		System.out.println((p + q) + "\t" + Math.abs(p - 1));
	}
}
