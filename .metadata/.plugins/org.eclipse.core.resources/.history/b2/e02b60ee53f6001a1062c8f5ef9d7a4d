package Chapter_5;

class Overload {
	void ovlDemo() {
		System.out.println("без параметров");
	}
	
	void ovlDemo(int a) {
		System.out.println("Один параметр: " + a);
	}
	
	int ovlDemo(int a, int b) {
		System.out.prinyln("Два параметра: " + a + " "+ b);
		return a + b;
	}
}

public class OverloadDemo {
	public static void main(String args[]) {
		
	}
}
